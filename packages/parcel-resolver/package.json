{
  "name": "@plasmohq/parcel-resolver",
  "version": "0.9.0",
  "description": "Plasmo Parcel Resolver",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "run-s build:polyfills dev:resolver",
    "dev:resolver": "tsup --watch src/index.ts",
    "build": "run-s build:*",
    "build:resolver": "tsup src/index.ts --minify --clean",
    "build:polyfills": "node index.mjs",
    "prepublishOnly": "pnpm build"
  },
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "homepage": "https://docs.plasmo.com/",
  "engines": {
    "parcel": ">= 2.7.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PlasmoHQ/plasmo.git"
  },
  "devDependencies": {
    "@plasmo/config": "workspace:*",
    "@plasmo/utils": "workspace:*",
    "assert": "2.0.0",
    "browserify-zlib": "0.2.0",
    "buffer": "6.0.3",
    "console-browserify": "1.2.0",
    "constants-browserify": "1.0.0",
    "crypto-browserify": "3.12.0",
    "domain-browser": "4.22.0",
    "events": "3.3.0",
    "https-browserify": "1.0.0",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "process": "0.11.10",
    "punycode": "2.1.1",
    "querystring-es3": "0.2.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "string_decoder": "1.3.0",
    "timers": "0.1.1",
    "timers-browserify": "2.0.12",
    "esbuild": "0.16.8",
    "tiny-glob": "0.2.9",
    "tsup": "6.5.0",
    "tty-browserify": "0.0.1",
    "url": "0.11.0",
    "util": "0.12.5",
    "vm-browserify": "1.1.2"
  },
  "dependencies": {
    "@parcel/core": "2.8.2",
    "@parcel/hash": "2.8.2",
    "@parcel/plugin": "2.8.2",
    "@parcel/types": "2.8.2",
    "got": "12.5.3"
  }
